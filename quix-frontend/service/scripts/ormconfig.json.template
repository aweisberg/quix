{
  "type": "mysql",
  "host": ":dbhost:",
  "port": :dbport:,
  "username": ":dbuser:",
  "password": ":dbpass:",
  "database": ":dbname:",
  "entities": ["src/**/*.entity{.ts,.js}"],
  "migrations": ["./migrations/*.ts"],
  "synchronize": false,
  "cli": {
    "migrationsDir": "./migrations"
  },
  logging": true
}